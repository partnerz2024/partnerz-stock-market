{
  "name": "partnerz-stock-market",
  "version": "1.0.0",
  "description": "파트너즈 증권 거래소 - 동아리 축제 모의투자 앱",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d client/build"
  },
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "sqlite3": "^5.1.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0",
    "gh-pages": "^6.0.0"
  },
  "keywords": ["stock", "trading", "simulation", "pwa"],
  "author": "파트너즈 동아리",
  "license": "MIT",
  "homepage": "https://partnerz2024.github.io/partnerz-stock-market"
}
